<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.recharge.mapper.IChannelOrderSupRelationMapper">

    <resultMap id="BaseResultMap" type="com.recharge.domain.ChannelOrderSupRelation" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="channel_order_id" property="channelOrderId" jdbcType="VARCHAR" />
        <result column="sup_order_id" property="supOrderId" jdbcType="VARCHAR" />
        <result column="sup_id" property="supId" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="ext_info" property="extInfo" jdbcType="VARCHAR" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, channel_order_id, sup_order_id, sup_id, create_time,ext_info
    </sql>

    <select id="selectBySupOrderId" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List"/> from t_channelorder_sup_relation where sup_order_id=#{supOrderId}
    </select>

    <update id="updateSupOrderId">
        update t_channelorder_sup_relation SET
        sup_order_id = #{ChannelOrderSupRelation.supOrderId}
        ,create_time=#{ChannelOrderSupRelation.createTime}
        ,sup_id=#{ChannelOrderSupRelation.supId}
        where channel_order_id =#{ChannelOrderSupRelation.channelOrderId}
    </update>

</mapper>